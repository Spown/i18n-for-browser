{
  "name": "i18n-for-browser",
  "version": "0.9.5",
  "description": "Implementation of i18n-node designed for client-side.",
  "author": "dangreen",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "browser": "lib/umd.js",
  "scripts": {
    "lint": "eslint --cache 'src/**/*.js' 'test/**/*.js'",
    "test": "npm run lint && nyc mocha -b",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/i18n-for-browser.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/i18n-for-browser/issues"
  },
  "keywords": [
    "template",
    "i18n",
    "l10n",
    "browser",
    "client",
    "client-side"
  ],
  "dependencies": {
    "assert": "^1.4.1",
    "make-plural": "^4.0.1",
    "math-interval-parser": "^2.0.0",
    "messageformat": "^1.0.2",
    "mustache": "^2.3.0",
    "sprintf-js": ">=1.0.3",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.26.0",
    "escope": "^3.6.0",
    "eslint": "^4.10.0",
    "eslint-config-trigen": "^1.2.4",
    "mocha": "*",
    "nyc": "^11.3.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-commonjs": "^8.2.5",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "should": "^13.1.3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "iOS > 7",
              "Android > 4.4",
              "not OperaMini all"
            ]
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "files": [
    "lib"
  ]
}
